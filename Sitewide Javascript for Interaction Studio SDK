                    }

                    if (mostRecentCollection) {
                        actionEvent.user.attributes.MostRecent_Collection = mostRecentCollection;
                    }
                   
                    if (mostRecentState) {
                        actionEvent.user.attributes.MostRecent_State = mostRecentState;
                    }
                   
                }
               
                return actionEvent;
}
