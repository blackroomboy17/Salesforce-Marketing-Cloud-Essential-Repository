if (typeof (Evergage.FlickerDefender || {}).setPageMatchTimeout === "function") {
    Evergage.FlickerDefender.setPageMatchTimeout(2500);
}

if (typeof (Evergage.FlickerDefender || {}).setRedisplayTimeout === "function") {
    Evergage.FlickerDefender.setRedisplayTimeout(5000);
}

const getCookieDomain = () => {
    let currentDomain = window.location.hostname;
    if (currentDomain === "<insert UAT website URL>") {
        return "<insert UAT website URL>";
    } else if (currentDomain === "<insert Production website URL>") {
        return "<insert Production website URL>";
    } else if (currentDomain === "<insert developer website URL>") {
        return "<insert developer website URL>";
    }
}
const currentCookieDomain = getCookieDomain();

Evergage.init({
    account: "<SFMC Parent Org SID Name>", //TODO: Remove this when deploy on production
dataset: "<SFMC Child Org SID Name>", //TODO: Remove this when deploy on production
