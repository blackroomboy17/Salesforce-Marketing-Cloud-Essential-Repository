        global: {
            onActionEvent: (actionEvent) => {
            actionEvent.user = actionEvent.user || {};
actionEvent.user.attributes = actionEvent.user.attributes || {};

                if(window.accountDetails) {
    const cwsWebUserId = window.accountDetails.eiid;
    const languagePreference = window.accountDetails.languagePreference;
    const mostRecentVinNumber = window.accountDetails.vinNumber;
    const mostRecentAccountNumber = window.accountDetails.accountNumber;
    const mostRecentType = window.accountDetails.isRetail==true ? "Retail"
                                       : (window.accountDetails.isLease==true ? "Lease" : undefined);
    const mostRecentStartDate = window.accountDetails.isRetail==true ? accountDetails.contractStartDate
                                       : (window.accountDetails.isLease==true ? accountDetails.leaseStartDate : undefined);
    const mostRecentEndDate = window.accountDetails.isRetail==true ? accountDetails.contractEndDate
                                       : (window.accountDetails.isLease==true ? accountDetails.leaseEndDate : undefined);
    const mostRecentTerm = window.accountDetails.isRetail==true ? accountDetails.contractTerm
                                       : (window.accountDetails.isLease==true ? accountDetails.leaseTerm : undefined);
    const mostRecentVehicleYear = window.accountDetails.vehicleYear;
    const mostRecentVehicleMake = window.accountDetails.vehicleMake;
    const mostRecentVehicleModel = window.accountDetails.vehicleModel;

                    const mostRecentLegalStatus = window.accountDetails.personaLegalStatus;
